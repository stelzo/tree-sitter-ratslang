{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ratslang",
      "camelcase": "Ratslang",
      "title": "Ratslang",
      "scope": "source.ratslang",
      "file-types": [
        "rl"
      ],
      "injection-regex": "^ratslang$",
      "class-name": "TreeSitterRatslang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Configuration language for Robots",
    "authors": [
      {
        "name": "Christopher Sieh",
        "email": "stelzo@steado.de"
      }
    ],
    "links": {
      "repository": "https://github.com/stelzo/tree-sitter-ratslang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}